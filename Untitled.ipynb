    "def make_iterable():\n",
    "    ch = input()\n",
    "    if ch == '0':\n",
    "        return ch\n",
    "    yield ch\n",
    "    \n",
    "for ch in chain.from_iterable(make_iterable()):\n",
    "    print(ch)"
